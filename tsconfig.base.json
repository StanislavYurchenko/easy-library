{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "inlineSources": false,
    "inlineSourceMap": false,
    "declaration": false,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2017", "dom", "es2018.promise"],
    "skipLibCheck": true,
    "strictNullChecks": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@libs/api-interface": ["libs/api-interface/"],
      "@server-nest/configs": ["apps/server-nest/src/app/configs/index"],
      "@server-nest/modules": ["apps/server-nest/src/app/modules/index"]
    },
    "strict": true
  },
  "exclude": ["node_modules", "tmp", "dist"]
}
