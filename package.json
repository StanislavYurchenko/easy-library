  "scripts": {
    "nx": "nx",
    "build": "npm run build:client && npm run build:server",
    "serve": "npm run serve:client && npm run serve:server",
    "lint": "npm run lint:client && npm run lint:server",
    "test": "npm run test:client && npm run test:server",
    "build:client": "nx build client-angular",
    "build:server": "nx build server-nest",
    "serve:client": "nx serve client-angular",
    "serve:server": "nx serve server-nest",
    "lint:client": "nx lint client-angular",
    "lint:server": "nx lint server-nest",
    "test:client": "nx test client-angular",
    "test:server": "nx test server-nest",
    "clearCache": "rm -rf ./node_modules/.cache/nx",
    "eslint:github-action": "npm run lint"
